{
  "name": "my_web_docs",
  "version": "1.0.0",
  "description": "This is my personnal web docs",
  "scripts": {
    "build:pdf:open": "md-to-pdf src/docs/release_resume/README.md --config-file ./src/configs/md-to-pdf_config.js",
    "build:pdf:close": "md-to-pdf src/docs/personal/resume.md --config-file ./src/configs/md-to-pdf_config.js",
    "print:html": "node src/configs/md-to-html.js src/docs/release_resume/README.md",
    "md:svg": " mjc -f src/docs/my-dev/engineer-judge-app/api/diagram/ER.md -o SVG"
  },
