    "commander": "^10.0.0",
    "html-pdf": "^3.0.1",
    "marked": "^4.2.12",
    "md-to-pdf": "^5.0.0",
    "mermaid-js-converter": "^1.0.13",
    "module_name": "^1.0.0",
    "typescript": "^4.9.4"
  }
}
